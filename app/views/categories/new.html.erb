<%= form_for @category do |f| %>

   <div class="#">
      <%= f.text_field :name  %>
      <%= f.label :name, "Name of Category" %>
    </div>

    <div class="#">
      <%= f.file_field :img_src  %>
      <%= f.label :img_src, "Picture" %>
    </div>

    <div class="#">
      <%= f.submit "Submit" %>
    </div>

<% end %>